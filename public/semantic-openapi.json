{
  "$schema": "https://spec.actionschema.com/actionschema.json",
  "description": "This one is rather complex. It's a schema that defines a schema for a semantic openapi, I think.",
  "type": "object",
  "required": ["$schema", "type", "additionalProperties", "properties"],
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "type": "string",
      "enum": ["https://spec.actionschema.com/semantic-openapi.json"],
      "default": "https://spec.actionschema.com/semantic-openapi.json"
    },
    "type": {
      "type": "string",
      "enum": ["object"],
      "default": "object"
    },
    "additionalProperties": {
      "type": "boolean",
      "enum": [false],
      "default": false
    },
    "properties": {
      "description": "The operations. Each operation is defined under a key of the operationId, where the value contains all information of that operation.",
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^[a-zA-Z]+[a-zA-Z0-9]*$": {
          "$ref": "https://spec.actionschema.com/semantic-operation.json"
        }
      }
    },
    "definitions": {
      "type": "object",
      "additionalProperties": {
        "$ref": "https://spec.actionschema.com/actionschema.json"
      }
    }
  }
}
